package myVo;

public class SearchReviewByTextVo {
	private int roomIdx;
	private int reviewIdx;
	private String review;
	private int userIdx;
	private int score;
	private String wirttenDate;
	
	public SearchReviewByTextVo(int roomIdx, int reviewIdx, String review, int userIdx, int score, String wirttenDate) {
		this.roomIdx = roomIdx;
		this.reviewIdx = reviewIdx;
		this.review = review;
		this.userIdx = userIdx;
		this.score = score;
		this.wirttenDate = wirttenDate;
	}
}
